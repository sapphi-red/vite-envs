{
  "name": "@vite-runtime/standalone",
  "private": true,
  "type": "module",
  "bin": {
    "vite-env": "./bin/vite-env.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch --format=esm --dts --entry src/index.ts --entry src/config.ts",
    "build": "tsup --format=esm --dts --entry src/index.ts --entry src/config.ts"
  },
  "dependencies": {
    "@hattip/adapter-node": "^0.0.42",
    "connect": "^3.7.0",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/http-proxy": "^1.17.14",
    "tsup": "^8.0.2",
    "vite": "^5.1.3"
  },
  "peerDependencies": {
    "vite": "^5.1.0"
  }
}
